@model List<WSite_ShowRoom_CtyThoiTrang.Models.tb_OrderDetail>

<h2>Order Details</h2>

<h3>Order Information:</h3>
<p>Order ID: @ViewBag.Order.OrderId</p>
<p>Customer Name: @ViewBag.Order.CustomerName</p>
<!-- Thêm các trường khác tùy thuộc vào cấu trúc của tb_Order -->

<h3>Order Details:</h3>
<table>
    <tr>
        <th>Product ID</th>
        <th>Price</th>
        <th>Quantity</th>
        <!-- Thêm các cột khác tùy thuộc vào cấu trúc của tb_OrderDetail -->
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ProductId</td>
            <td>@item.Price</td>
            <td>@item.Quantity</td>
            <!-- Thêm các cột khác tùy thuộc vào cấu trúc của tb_OrderDetail -->
        </tr>
    }
</table>

