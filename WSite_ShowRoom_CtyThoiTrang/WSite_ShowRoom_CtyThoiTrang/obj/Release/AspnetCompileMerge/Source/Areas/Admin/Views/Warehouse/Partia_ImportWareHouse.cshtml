@model WSite_ShowRoom_CtyThoiTrang.Models.tb_KhoNhap

@using (Html.BeginForm("Partia_ImportWareHouse", "WareHouse", FormMethod.Post, new { })) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>tb_KhoNhap</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
      

        <div class="form-group">
            @Html.LabelFor(model => model.MaPhieuNhap, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.MaPhieuNhap, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.MaPhieuNhap, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Danh mục</label>
            <div class="col-md-10">
                @Html.DropDownListFor(x => x.IdKho, ViewBag.Kho as SelectList, "-Chọn kho -", new { @class = "form-control", @placeholder = "Kho hàng" })
                @Html.ValidationMessageFor(x => x.IdKho, null, new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

